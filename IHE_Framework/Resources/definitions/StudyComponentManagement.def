(0x00080106,3C,DT,1)                                "Context Group Version"          : "PRESENT 0x0008010F and attribute is requested"
(0x00080107,3C,DT,1)                                "Context Group Local Version"    : "VALUE 0x0008010B = Y OR PRESENT 0x0008010F and attribute is requested"
(0x0008010D,3C,UI,1)                                "Context Group Extension Creator UID" : "VALUE 0x0008010B = Y and attribute is requested"
(0x0008010B,3C,CS,1,E,"N"
                    | "Y")                          "Context Group Extension Flag"   : "Present if attribute is requested"
(0x0008010F,3C,CS,1)                                "Context Identifier"             : "Present if attribute is requested"
ENDDEFINE

