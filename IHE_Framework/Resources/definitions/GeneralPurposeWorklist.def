(0x00080102,1C,SH,1)                                "Coding Scheme Designator"       : "Requered if the attributes is requested"
(0x00080104,1C,LO,1)                                "Code Meaning"                   : "Requered if the attributes is requested"
ENDDEFINE

DEFINE MACRO "SOP Instance Reference Macro General Purpose Worklist SOP Class C-FIND SCP"
(0x00081115,1C,SQ,1,
 >(0x00081199,1C,SQ,1,
   >(0x00081150,1C,UI,1)                            "Referenced SOP Class UID"       : "Requered if the attributes is requested"
   >(0x00081155,1C,UI,1)                            "Referenced SOP Instance UID"    : "Requered if the attributes is requested"
  )                                                 "Referenced SOP Sequence"        : "Requered if the attributes is requested"
 >(0x0020000E,1C,UI,1)                              "Series Instance UID"            : "Requered if the attributes is requested"
 >(0x00080054,2C,AE,1:n)                            "Retrieve AE Title"              : "PRESENT 0x00880130 OR PRESENT 0x00880140 and the attribute is requested"
 >(0x00880130,2C,SH,1)                              "Storage Media File-Set ID"      : "NOT PRESENT 0x00080054 and the attribute is requested"
 >(0x00880140,2C,UI,1)                              "Storage Media File-Set UID"     : "NOT PRESENT 0x00080054 and the attribute is requested"
)                                                   "Referenced Series Sequence"     : "Requered if the attributes is requested"
(0x0020000D,1C,UI,1)                                "Study Instance UID"             : "Requered if the attributes is requested"
ENDDEFINE

DEFINE MACRO "Code Sequence Macro General Purpose Worklist SOP Class C-FIND SCU"
(0x00080100,3,SH,1)                                 "Code Value"
(0x00080102,3,SH,1)                                 "Coding Scheme Designator"
(0x00080104,3,LO,1)                                 "Code Meaning"
ENDDEFINE

DEFINE MACRO "SOP Instance Reference Macro General Purpose Worklist SOP Class C-FIND SCU"
(0x00081115,3,SQ,1,
 >(0x00080054,3,AE,1:n)                             "Retrieve AE Title"
 >(0x00081199,3,SQ,1,
   >(0x00081150,3,UI,1)                             "Referenced SOP Class UID"
   >(0x00081155,3,UI,1)                             "Referenced SOP Instance UID"
  )                                                 "Referenced SOP Sequence"
 >(0x0020000E,3,UI,1)                               "Series Instance UID"
 >(0x00880130,3,SH,1)                               "Storage Media File-Set ID"
 >(0x00880140,3,UI,1)                               "Storage Media File-Set UID"
)                                                   "Referenced Series Sequence"
(0x0020000D,3,UI,1)                                 "Study Instance UID"
ENDDEFINE

